<keymap version="1" name="GNOME copy" parent="Default for GNOME">
  <action id="GotoNextError">
    <keyboard-shortcut first-keystroke="shift ctrl alt end" />
  </action>
  <action id="GotoPreviousError">
    <keyboard-shortcut first-keystroke="shift ctrl alt home" />
  </action>
  <action id="Scala.ShowImplicits">
    <keyboard-shortcut first-keystroke="shift ctrl alt equals" />
    <keyboard-shortcut first-keystroke="shift ctrl alt add" />
  </action>
  <action id="ToggleZenMode">
    <keyboard-shortcut first-keystroke="shift ctrl alt f11" />
  </action>
</keymap>